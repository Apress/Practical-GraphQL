{
    "compilerOptions": {
        "target": "es5",
        "lib": [
            "dom",
            "es6"
        ], 
        "module": "commonjs",
        "rootDir": "src",
        "outDir": "dist",
        "removeComments": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true
    }
}
